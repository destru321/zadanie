@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}

<div>
    <table class="table">
        <thead>
            <tr>
                <th>id</th>
                <th>name</th>
                <th>phone</th>
            </tr>
        </thead>
        <tbody>
            @if(Model.Users != null)
             {
                 foreach(var user in Model.Users)
                 {
                    <tr>
                        <td>@user.Id</td>
                        <td>@user.name</td>
                        <td>@user.phone</td>
                    </tr>
                 }
             }
                
        </tbody>
    </table>
</div>

